{
  "name": "mixtape-front",
  "version": "1.0.0",
  "description": "Mixtape Front",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btomashvili/mixtape-front.git"
  },
  "keywords": [],
