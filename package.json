  "license": "MIT",
  "bugs": {
    "url": "https://github.com/btomashvili/mixtape-front/issues"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open  --mode development --config webpack/client/dev.js --progress --profile --colors",
    "start:local": "NODE_ENV=development_local webpack-dev-server --open --mode development --config webpack/client/dev.js --progress --profile --colors",
    "build:testing": "NODE_ENV=testing webpack --mode development --config webpack/client/testing.js --progress --profile --colors",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack/client/production.js --progress --profile --colors",
    "start:dist": "node ./www.js",
    "build:ssr:testing": "NODE_ENV=testing SSR_MODE=true webpack --mode development --config webpack/client/testing.js --progress --profile --colors && NODE_ENV=development_local webpack --mode production --config webpack/server/testing.js --progress --profile --colors",
    "build:ssr:prod": "NODE_ENV=production SSR_MODE=true webpack --mode production --config webpack/client/production.js --progress --profile --colors && NODE_ENV=production webpack --mode production --config webpack/server/production.js --progress --profile --colors",
    "start:ssr:dist": "node ./dist/index.js",
    "pm2_ssr_testing": "NODE_ENV=testing DEBUG=* CONFIG=testing pm2 start ./dist/index.js --name mixtape_front_ssr_8003",
    "pm2_delete_ssr_testing": "pm2 delete mixtape_front_ssr_8003",
    "pm2_ssr_prod": "NODE_ENV=production DEBUG=* CONFIG=production pm2 start ./dist/index.js --name mixtape_front_ssr_8003",
    "pm2_delete_ssr_prod": "pm2 delete mixtape_front_ssr_8003",
    "format": "prettier --write \"src/**/*.js\" --single-quote --no-semi --trailing-comma=all",
    "precommit": "lint-staged",
    "lint": "eslint ./src",
    "build:full": "npm run build:prod && npm:ssr:prod"
  },
  "lint-staged": {
    "*.js": [
      "git add",
      "eslint"
    ]
  },
  "pre-commit": [
    "precommit"
  ],
